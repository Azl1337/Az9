<Window x:Class="GG_Suicide_Iz_Wae.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:GG_Suicide_Iz_Wae"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition />
            <RowDefinition Height="40"/>
        </Grid.RowDefinitions>
        <Frame x:Name="mainFrame" Grid.Row="0" NavigationUIVisibility="Hidden">
        </Frame>
        <TextBlock x:Name="tbTimeLeft" Grid.Row="1" TextWrapping="Wrap" Style="{DynamicResource text_block}">
        </TextBlock>
    </Grid>
</Window>
